generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model leadsvendedores {
  id          Int     @id @default(autoincrement())
  nombre      String?
  whatsapp    String?
  correo      String?
  status      String?
  agencia     String?
  ciudad      String?
  comentarios String?
}

model propiedades {
  id      Int     @id @default(autoincrement())
  nombre  String?
  tipo    String?
  user    user?   @relation(fields: [userId], references: [id])
  userId  Int?
  imagen  String?
  estado  String?
  ciudad  String?
  banos   String?
  cuartos String?
  metros  Int?
}

model user {
  id          Int           @id @default(autoincrement())
  nombre      String?
  email       String?       @unique
  password    String
  telefono    String?
  agencia     String?
  propiedades propiedades[]
}
